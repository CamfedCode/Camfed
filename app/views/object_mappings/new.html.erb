<%= form_for(@object_mapping, :url => survey_object_mappings_path(@survey)) do |f|%>
	
	<%= f.hidden_field :survey_id %>
  <div class="field">
    <%= f.label :sf_object_type %><br />
    <%= f.select :sf_object_type, @sf_object_types, :prompt => true %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>