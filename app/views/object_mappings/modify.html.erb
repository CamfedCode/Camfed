<%= title "Modify Mapping for #{@survey.name}", true %>


<%= form_for(@object_mapping, :url => survey_object_mappings_path(@survey)) do |f|%>	

	<%= f.hidden_field :survey_id %>

	<%= f.select :sf_object_type, @sf_object_types.collect{|an_object| [an_object.label, an_object.name]}, :prompt => 'Select Salesforce Object' %>


	<%= f.submit 'Proceed' %>

<% end %>